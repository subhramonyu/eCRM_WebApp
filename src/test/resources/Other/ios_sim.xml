<?xml version="1.0" encoding="UTF-8"?>

<suite name = "Fluke Connect Test Suite">

<test name = "iOS Tests" preserve-order="false">
<parameter name = "platformName" 				value = "iOS" />
<parameter name = "platformVersion" 			value = "11.2" />
<parameter name = "app" 						value ="/ele/sim/elektra.app" />
<parameter name = "bundleID" 					value = "com.fluke.mobility.elektra" />
<parameter name = "deviceName" 					value = "iPhone 7" />
<parameter name = "driverName" 					value = "iOS" />
<parameter name = "environmentName" 			value = "Dev" />
<parameter name = "userName" 					value = "smoketests@yopmail.com" />
<parameter name = "password" 					value = "smoketests@yopmail.com" />
<parameter name = "appInstallFlag"				value = "false"  />
<parameter name = "handleUpgradeAlert"			value = "true"  />

<parameter name = "wdaLocalPort"			value = "8110" />

<groups>
<run>
<include name="ios_smoke_tests" />
</run>
</groups>

<packages>
<package name="com.fluke.connect.app.*" />
</packages>
</test>

</suite>