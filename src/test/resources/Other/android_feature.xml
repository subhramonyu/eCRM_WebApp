<?xml version="1.0" encoding="UTF-8"?>

<suite name = "Fluke Connect Test Suite">

<test name = "${test_name}" preserve-order="false">

<parameter name = "driverName" 					value = "Android" />
<parameter name = "platformName" 				value = "Android" />
<parameter name = "deviceName" 					value = "${device_name}" />
<parameter name = "deviceUdid" 					value = "${udid}" />
<parameter name = "platformVersion" 				value = "${platform_version}" />
<parameter name = "appInstallFlag"				value = "${app_install_flag}"  />
<parameter name = "app" 	  	            		    value = "${app_location}" />
<parameter name = "appPackage" 					value = "com.fluke.deviceApp" />
<parameter name = "appActivity" 					value = "com.fluke.deviceApp.LoginActivity" />
<parameter name = "environmentName" 				value = "${environment_name}" />
<parameter name = "userName" 					value = "${user_name}" />
<parameter name = "password" 					value = "${password}" />

<groups>
<run>
<include name="${group_name}" />
</run>
</groups>

<packages>
<package name = "com.fluke.connect.app.*" />
</packages>

</test>

</suite>