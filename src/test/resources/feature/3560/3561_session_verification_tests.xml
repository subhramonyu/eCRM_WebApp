<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name = "Fluke Connect Test Suite">

<test name = "Web 3560 Regression Tests">

<parameter name = "driverName"       		value = "Web" />
<parameter name = "browserName"      		value = "Chrome" />

<parameter name = "environmentName" 	    		value = "PreProduction" />
<parameter name = "url"             			value = "https://preprod.connect.fluke.com" />

<parameter name = "useExistingData" 			value = "true" />

<parameter name = "activeSessionStartTime3560"			value = "02/18/2019, 17:07:29"  />
<parameter name = "completedSessionStartTime3560"			value = "02/18/2019, 13:30:31"  />

<parameter name = "toggleFeature3560"					value = "false"  />
<parameter name = "activeSessionGatewayName3560"			value = "Fluke 3502 Gateway (C2AE5)"  />
<parameter name = "completedSessionGatewayName3560"		value = "Fluke 3502 Gateway (C2AE5)"  />

<groups>
<run>

<include name = "3560_session_tests_web" />


</run>
</groups>

<packages>
<package name="com.fluke.connect.app.*" />
</packages>

</test> 

<listeners>
<listener class-name="com.fluke.connect.app.utils.TestResultsReport" />
</listeners>

</suite>